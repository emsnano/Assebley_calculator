WindowManager:
    StartWindow:
    WorkWithComponentsWindow:


<StartWindow>
    name:"Startowe"
    
    MDFloatLayout:

        MDLabel:
            text:'Nanotech Elektronik Polska'
            font_size: 25
            pos_hint: {"x":0.15, "y":0.65, "bottom":1}
            size_hint: 0.5, 0.07
        MDLabel:
            text:'Kalkulator montażu komponentów V1.0'
            font_size: 17
            pos_hint: {"x":0.15, "y":0.58, "bottom":1}
            size_hint: 0.5, 0.07
        
                #Przyciski wyboru wersji LZ Excela
        MDLabel_custom:
            text: 'LZ V2.2'
            size_hint: 0.13, 0.07
            halign: "center"
            pos_hint: {"x":0.745, "y":0.345, "bottom":1}
        Check_LZ:
            pos_hint: {"x":0.79, "y":0.31, "bottom":1}
            on_active: root.change_LZ('Old')

        MDLabel_custom:
            text: 'LZ V3.0'
            size_hint: 0.13, 0.07
            halign: "center"
            pos_hint: {"x":0.825, "y":0.345, "bottom":1}
        Check_LZ:
            active: True
            pos_hint: {"x":0.87, "y":0.31, "bottom":1}
            on_active: root.change_LZ('New')
        #Przycisk pokazu instrukcji Light 
        MDRoundFlatButton:
            pos_hint: {"x":0.785, "y":0.225, "bottom":1}
            # size_hint: 0.14, 0.08
            text: "Instrukcja użytkownika"  
            on_release: root.Pokaz_instarukcje()
        MDRoundFlatButton:
            pos_hint: {"x":0.792, "y":0.17, "bottom":1}
            # size_hint: 0.14, 0.08
            text: "Instrukcja developera"  
            on_release: root.Pokaz_instarukcje()
        MDFillRoundFlatButton :
            pos_hint: {"x":0.819, "y":0.095, "bottom":1}
            text: "Kalkulator"
            on_release:
                app.root.current = "Komponentowe"
                root.manager.transition.direction = "left"
        MDFillRoundFlatButton :
            pos_hint: {"x":0.810, "y":0.04, "bottom":1}
            text: "Nowa wycena"
            on_release:
                app.root.current = "Komponentowe"
                root.manager.transition.direction = "left"





